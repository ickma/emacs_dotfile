   (quote
    (("gnu" . "http://elpa.gnu.org/packages/")
     ("melpa" . "https://melpa.org/packages/"))))
 '(package-selected-packages
   (quote
    (js2-refactor markdown-mode nyan-mode solarized-theme idea-darkula-theme js2-mode)))
